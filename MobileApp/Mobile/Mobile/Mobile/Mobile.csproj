    </EmbeddedResource>
    <EmbeddedResource Update="SingleTrackPlay.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Testowy.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>

    </Compile>
    <Compile Update="MyOrders.xaml.cs">
      <DependentUpon>MyOrders.xaml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="AboutUs.xaml">
